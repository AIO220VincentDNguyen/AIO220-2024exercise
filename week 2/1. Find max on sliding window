#Exercise 1: Find the maximum value in a sliding window of size k
def maxSlidingWindow(nums, k):
    if k < 1:
        return []
    for i in range(len(nums) - k + 1):
        #range(len(nums) - k + 1): Hàm range tạo ra một chuỗi các số nguyên từ 0 đến ``len(nums) - k(bao gồm cả 0 và loại trừ len(nums) - k).
        window = nums[i:i + k]
        max_val = max(window)
        before_window = nums[:i]
        after_window = nums[i + k:]
        print(f"{before_window}, [{', '.join(map(str, window))}], {', '.join(map(str, after_window))} => max {max_val}")
    return
num_list = [3, 4, 5, 1, -44, 5, 10, 12, 33, 1]
k = 3
maxSlidingWindow(num_list, k)